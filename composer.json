{
  "name": "tourze/currency-manage-bundle",
  "description": "货币管理",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "brick/money": "^0.10",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^3.1 || ^4",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/enum-extra": "~0.0.5"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
